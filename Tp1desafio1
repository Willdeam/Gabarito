        <title>Ordem inversa2</title>
        <script>
        var numeros = new Array(20);
        function vetorAoQuadrado(){
        for(var i = 0; i<numeros.length; i+=2) {
             numeros[i] = i;
        }
        document.getElementById("resultA1").innerHTML = numeros;
        }
        </script>
    </head>
    <body>
        <h1>h1</h1>
        <h2>h2</h2>
        <p id="resultA1">p1<p>
        <p id="resultA">p</p>
        <button type="button" onclick="vetorAoQuadrado()">Aperta</button>
